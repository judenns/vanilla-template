{
	"name": "template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --write ./src",
		"format": "biome format --write ./src",
		"format:css": "prettier --write \"src/**/*.css\"",
		"format:html": "prettier --write \"src/**/*.html\"",
		"format:all": "pnpm format && pnpm format:css && pnpm format:html",
		"check": "biome check --write ./src",
		"clean": "rm -rf dist",
		"clean:all": "rm -rf dist node_modules pnpm-lock.yaml",
		"reinstall": "pnpm run clean:all && pnpm install"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.6",
		"autoprefixer": "^10.4.22",
		"cssnano": "^7.1.2",
		"postcss": "^8.5.6",
		"postcss-import": "^16.1.1",
		"postcss-preset-env": "^10.4.0",
		"prettier": "^3.7.4",
		"vite": "^7.2.2"
	},
	"browserslist": [
		"defaults and fully supports es6-module",
		"not dead"
	]
}
